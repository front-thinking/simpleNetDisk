                <ul class="nav navbar-nav navbar-right ">
                    <li>
                        <a href="#">关于</a>
                    </li>
                    <li>
                        <a href="#">帮助</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <ol class="breadcrumb">
            <li><a href="#/">Home</a></li>
        </ol>
        <div class="col-md-12">

            <div class="file-list-container">
                <table id="file-list" class="table table-striped table-bordered" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <td width="30">
                                <input type="checkbox" class="select-all">
                            </td>
                            <td>文件名</td>
                            <td width="200">大小</td>
                            <td width="200">修改时间</td>
                        </tr>
                    </thead>
                    <tbody>

                    </tbody>
                </table>
                <div class="file-action">
                    <button class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;新建文件夹</button>
                     <div id="uploader" class="wu-example">
                        <!--用来存放文件信息-->
                        <div class="btns">
                            <div id="picker"><i class="fa fa-upload"></i>&nbsp;上传文件</div>
                            <button id="ctlBtn" class="btn btn-default">开始上传</button>
                        </div>
                    </div>
                     <div id="thelist" class="uploader-list"></div>
                </div>
            </div>
        </div>
    </div>
   

    <div id="mm" class="easyui-menu" style="width:120px;display:none">
        <div action="open">打开</div>
        <div class="menu-sep"></div>
        <div action="rename">重命名</div>
        <div>移动到</div>
        <div>复制到</div>
        <div class="menu-sep"></div>
        <div action="delete">删除</div>
    </div>
    <div id="mm2" class="easyui-menu" style="width:120px;display:none">
        <div>列表视图</div>
        <div>略缩图视图</div>
        <div class="menu-sep"></div>
        <div>按大小排序</div>
        <div>按时间排序</div>
        <div>按文件名排序</div>
        <div class="menu-sep"></div>
        <div>新建文件夹</div>
        <div>刷新</div>
    </div>
    <script src="./js/jquery.js"></script>
    <script type="text/javascript" src="./tools/easyUI/jquery.easyui.min.js"></script>
